<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<!-- 该文件默认情况下必须放在项目的src目录下 -->
<struts>
	<package name="data" extends="struts-default" namespace="/data">
		<action name="user_*" class="gz.itcast.f_data.UserAction"
			method="{1}">
			<result>/login.jsp</result>
		</action>

		<action name="user2_*" class="gz.itcast.f_data.UserAction2"
			method="{1}">
			<result>/login.jsp</result>
		</action>

		<action name="user3_*" class="gz.itcast.f_data.UserAction3"
			method="{1}">
			<result>/login.jsp</result>
		</action>
	</package>

</struts>



